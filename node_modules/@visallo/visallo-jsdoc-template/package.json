{
  "name": "@visallo/visallo-jsdoc-template",
  "version": "1.1.8",
  "description": "Visallo JSDoc 3 theme",
  "main": "publish.js",
  "scripts": {
    "test": "jsdoc -c fixtures/fixtures.conf.json",
    "sync": "browser-sync start -s ../fixtures-doc -f ../fixtures-doc --reload-delay 1000 --no-ui --no-notify",
    "watch": "watch-run -d 1000 -p tmpl/**,static/** \"npm run test\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/v5analytics/visallo-jsdoc-template.git"
  },
  "devDependencies": {
    "browser-sync": "latest",
    "jsdoc": "latest",
    "watch-run": "latest"
  },
  "author": "Jason Harwig <jason.harwig@visallo.com>",
  "contributors": [
    "Clement Moron <clement.moron@gmail.com>"
  ],
  "license": "Apache-2.0",
  "keywords": [
    "jsdoc",
    "template"
  ],
  "dependencies": {
    "escodegen": "^1.8.1"
  }
}
